<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>JointStateType</name>
  <description/>
  <version>1.0</version>
  <vendor/>

  <types>
    <typeDescription>
      <name>JointState</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>

        <featureDescription>
          <!-- "names"? -->
          <name>name</name>
          <description/>
          <rangeTypeName>uima.cas.StringArray</rangeTypeName>
        </featureDescription>

        <featureDescription>
          <name>seq</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>

        <featureDescription>
          <name>time</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>

        <featureDescription>
          <name>frameID</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>

        <featureDescription>
          <name>jointTrajectoryPoint</name>
          <description/>
          <rangeTypeName>JointTrajectoryPoint</rangeTypeName>
        </featureDescription>

      </features>
    </typeDescription>
  </types>

  <!-- Dependencies -->
  <imports>
    <import location="JointTrajectoryPoint.xml"/>
  </imports>

</typeSystemDescription>
