<?xml version="1.0" encoding="UTF-8"?>
<typeSystemDescription xmlns="http://uima.apache.org/resourceSpecifier">
  <name>ControllerInputType</name>
  <description/>
  <version>1.0</version>
  <vendor/>

  <types>
    <typeDescription>
      <name>ControllerInput</name>
      <description/>
      <supertypeName>uima.tcas.Annotation</supertypeName>
      <features>

        <featureDescription>
          <name>time</name>
          <description/>
          <rangeTypeName>uima.cas.Integer</rangeTypeName>
        </featureDescription>

        <featureDescription>
          <name>jointNames</name>
          <description/>
          <rangeTypeName>uima.cas.StringArray</rangeTypeName>
        </featureDescription>

        <featureDescription>
          <name>desired</name>
          <description/>
          <rangeTypeName>JointTrajectoryPoint</rangeTypeName>
        </featureDescription>

        <featureDescription>
          <name>actual</name>
          <description/>
          <rangeTypeName>JointTrajectoryPoint</rangeTypeName>
        </featureDescription>

        <featureDescription>
          <name>error</name>
          <description/>
          <rangeTypeName>JointTrajectoryPoint</rangeTypeName>
        </featureDescription>

        <featureDescription>
          <name>controllerType</name>
          <description/>
          <rangeTypeName>uima.cas.String</rangeTypeName>
        </featureDescription>

      </features>
    </typeDescription>
  </types>

  <!-- Dependencies -->
  <imports>
    <import location="JointTrajectoryPoint.xml"/>
  </imports>

</typeSystemDescription>
